{
  "manifest_version": 3,

  "name": "BRemover",
  "description": "BRemover can modify how certain webs work given the rules by the user.",
  "version": "1.0",

  "icons": {
    "128": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "permissions": ["tabs", "webNavigation", "storage", "downloads", "scripting"],
  "host_permissions": ["<all_urls>"]
}
