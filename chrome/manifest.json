{
  "manifest_version": 2,

  "name": "BRemover",
  "description": "This extension will remove classes from certain domains.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": ["tabs", "webNavigation", "*://*/*", "storage", "downloads"]
}
